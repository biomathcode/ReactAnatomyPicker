{
  "name": "react-anatomy-picker",
  "version": "0.0.4",
  "description": " Svg based Anatomy Picker build for react ",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/biomathcode/ReactAnatomyPicker#readme",
  "bugs": {
    "url": "https://github.com/biomathcode/ReactAnatomyPicker/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/biomathcode/ReactAnatomyPicker.git"
  },
  "author": "biomathcode <sharma.pratik2016@gmail.com>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "concurrently \"tsdown --watch\" \"pnpm storybook\" \"pnpm test\" ",
    "build": "tsdown",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "play": "vite --config playground/vite.config.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build",
    "deploy-storybook": "gh-pages -d storybook-static"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^10.1.2",
    "@storybook/addon-docs": "^10.1.2",
    "@storybook/addon-links": "^10.1.2",
    "@storybook/addon-themes": "^10.1.2",
    "@storybook/builder-vite": "^10.1.2",
    "@storybook/react-vite": "^10.1.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@tsconfig/strictest": "^2.0.8",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "bumpp": "^10.3.1",
    "concurrently": "^9.2.1",
    "gh-pages": "^6.3.0",
    "happy-dom": "^20.0.10",
    "storybook": "^10.1.2",
    "tsdown": "^0.16.4",
    "typescript": "^5.9.3",
    "vite": "npm:rolldown-vite@^7.2.5",
    "vitest": "^4.0.9"
  }
}